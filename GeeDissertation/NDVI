// Load a pre-computed Landsat composite for input.
var input = ee.Image('LANDSAT/LE7_TOA_1YEAR/2001');

// Define a region in which to generate a sample of the input.
var region = ee.FeatureCollection('USDOS/LSIB_SIMPLE/2017').
              filter(ee.Filter.eq('country_na', 'Thailand'));

// Display the sample region.
// // Make a handy variable of visualization parameters.
var ndwi = region.normalizedDifference(["B3","B5"]);
Map.setCenter(99.12,15.15, 10); 
Map.addLayer(ndwi, "NDWI", false);

